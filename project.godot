; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "PathFollow2D",
"class": "AutoPathFollow2D",
"language": "GDScript",
"path": "res://level_base/movement/auto_path_follow.gd"
}, {
"base": "Control",
"class": "LeveLController",
"language": "GDScript",
"path": "res://menu/level_controller.gd"
}, {
"base": "Resource",
"class": "LevelInfo",
"language": "GDScript",
"path": "res://level_info.gd"
}, {
"base": "Node2D",
"class": "Rotator",
"language": "GDScript",
"path": "res://level_base/movement/rotator.gd"
}, {
"base": "Resource",
"class": "WorldInfo",
"language": "GDScript",
"path": "res://world_info.gd"
}, {
"base": "Resource",
"class": "WorldList",
"language": "GDScript",
"path": "res://world_list.gd"
} ]
_global_script_class_icons={
"AutoPathFollow2D": "",
"LeveLController": "",
"LevelInfo": "",
"Rotator": "",
"WorldInfo": "",
"WorldList": ""
}

[application]

config/name="Orbiteno"
run/main_scene="res://menu/title.tscn"
config/icon="res://icon.png"

[autoload]

GameController="*res://game_controller.gd"

[display]

window/size/width=720
window/size/height=1080
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/Trail/plugin.cfg" )

[input_devices]

pointing/emulate_touch_from_mouse=true
pointing/emulate_mouse_from_touch=false

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0, 0, 0, 1 )
